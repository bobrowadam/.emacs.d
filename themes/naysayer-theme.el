 '(js2-warning ((t (:foreground "#ffaa00"))))
 '(highlight-numbers-number ((t (:foreground "#7ad0c6"))))
 '(default ((t (:foreground "#d1b897" :background "#062329" :weight normal)))))

(provide-theme 'naysayer)
