 '(rainbow-delimiters-depth-10-face ((t (:foreground "#E6DB74"))))
 '(rainbow-delimiters-depth-11-face ((t (:foreground "#FD971F"))))
 '(rainbow-delimiters-depth-12-face ((t (:foreground "#F92672"))))
 '(mode-line-buffer-id ((t (:foreground "#062329" :distant-foreground "#d1b897" :text "#d1b897" :weight bold))))
 '(mode-line ((t (:inverse-video unspecified :underline unspecified :foreground "#062329" :background "#d1b897" :box nil))))
 '(powerline-active1 ((t (:background "#d1b897" :foreground "#062329"))))
 '(powerline-active2 ((t (:background "#d1b897" :foreground "#062329"))))
 '(mode-line-inactive ((t (:inverse-video unspecified :underline unspecified :foreground "#d1b897" :background "#062329" :box nil))))
